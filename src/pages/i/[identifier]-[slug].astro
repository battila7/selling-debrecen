---
import { ListingDetailPage } from "../../components/pages";
import { getAllListings } from "../../listings";

export async function getStaticPaths() {
  const listings = await getAllListings("hu");

  return listings.map((listing) => ({
    params: {
      identifier: listing.baseId,
      slug: listing.slug,
    },
    props: {
      listing,
    },
  }));
}

const { listing } = Astro.props;
---

<ListingDetailPage listing={listing} />
